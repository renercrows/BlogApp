<main class='new-post'>
  <div>
    <h1>New Post</h1>
  </div>

  <%= form_with model: Post, url: user_posts_path do |f| %>
    <div>
      <%= f.label :title, "Caption/Title"%><br>
      <%= f.text_field :title %>
    </div>

    <div>
      <%= f.label :text %><br>
      <%= f.text_field :text %>
    </div>
    <%= f.submit "Create"%>
  <% end %>
</main>